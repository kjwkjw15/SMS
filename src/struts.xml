<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="home" extends="struts-default" namespace="/">
    	<action name="userRegister" class="ntts.action.UserAction" method="userRegister">
    		<result name="success">success_register.html</result>
    		<result name="error">success_register.html</result>
    	</action>
    	<action name="userLogin" class="ntts.action.UserAction" method="userLogin">
    		<result name="success">success_login.html</result>
    		<result name="error">success_login.html</result>
    	</action>
    </package>
</struts>